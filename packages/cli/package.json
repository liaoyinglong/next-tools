{
  "name": "@dune/cli",
  "version": "1.0.0",
  "scripts": {
    "dev": "pnpm run build-fast --watch",
    "build": "tsup src/cli.ts src/index.ts --clean --splitting",
    "prepublishOnly": "pnpm run build",
    "test": "pnpm run build && npm run test-only",
    "test-only": "vitest run",
    "build-fast": "pnpm run build --no-dts"
  },
  "type": "module",
  "types": "dist/index.d.ts",
  "bin": {
    "sq": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js"
    },
    "./cli": {
      "import": "./dist/cli.js"
    }
  },
  "dependencies": {
    "@dune/wasm": "workspace:*",
    "cac": "^6.7.14",
    "cli-table3": "^0.6.3",
    "debug": "^4.3.4",
    "fs-extra": "^10.1.0",
    "globby": "^13.1.2",
    "p-map": "^5.5.0",
    "prompts": "^2.4.2",
    "picocolors": "^1.0.0",
    "joycon": "^3.1.1"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/prompts": "^2.4.0",
    "tsup": "^6.2.3"
  }
}
